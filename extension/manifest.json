{
    "manifest_version": 3,
    "name": "MediShare",
    "version": "1.0",
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["encry.js","content.js"]
        }
    ]
}
